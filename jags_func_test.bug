
model {
  # Gaussian
  y_norm ~ dnorm(0, 1)
  ld_norm <- logdensity.norm(y_norm, 0, 1)
  
  # Binomial
  y_bin ~ dbin(0.5, 10)
  ld_bin <- logdensity.bin(y_bin, 0.5, 10)
  
  # Poisson
  y_pois ~ dpois(5)
  ld_pois <- logdensity.pois(y_pois, 5)
  
  # NegBin
  y_negbin ~ dnegbin(0.5, 5)
  ld_negbin <- logdensity.negbin(y_negbin, 0.5, 5)
}

